  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/MfFischer/makersai-studio.git"
  },
  "dependencies": {
    "react": "^19.2.0",
    "react-dom": "^19.2.0"
  },
  "devDependencies": {
    "@types/node": "^22.14.0",
    "@vitejs/plugin-react": "^5.0.0",
    "typescript": "~5.8.2",
    "vite": "^6.2.0",
    "concurrently": "^9.1.2"
  }
}
